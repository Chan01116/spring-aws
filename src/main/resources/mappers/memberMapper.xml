<?xml version="1.0" encoding="UTF-8"?>
<!-- 마이바티스용 문서 타입이다 -->

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myaws.myapp.persistance.MemberMapper">

<insert id="memberInsert" parameterType = "mv">
 insert into member(memberid,memberpwd,membername,membergender,memberbirth,memberaddr,memberphone,membereamil,memberhobby)
 values(#{memberid},#{memberpwd},#{membername},#{membergender},#{memberbirth},#{memberaddr},#{memberphone},#{membereamil},#{memberhobby})
</insert>




</mapper>